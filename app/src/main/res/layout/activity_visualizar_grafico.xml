<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VisualizarGrafico">
    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <LinearLayout
            android:id="@+id/resultados"
            android:layout_width="380dp"
            android:layout_height="wrap_content"
            android:layout_margin="15sp"
            android:orientation="vertical">


            <TextView
                android:id="@+id/txFuncao"
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:gravity="start"
                android:text=""
                android:textAlignment="viewStart"
                android:textSize="30sp" />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="15sp"
            android:orientation="vertical">
            <ImageButton
                android:id="@+id/bt_historico"
                android:layout_width="45dp"
                android:layout_height="48dp"
                android:layout_margin="5sp"
                android:layout_weight="1"
                android:backgroundTint="#FDFCFC"
                android:src="@drawable/rsz_1history"
                android:textSize="30sp"
                tools:ignore="TouchTargetSizeCheck" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/bt_back"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rsz_back"
                    android:text=""
                    android:textAllCaps="false"
                    android:textColor="#000000"

                    android:textSize="20dp" />

                <Button
                    android:id="@+id/bt_shift"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="shift"
                    android:textAllCaps="false"
                    android:textColor="#000000"

                    android:textSize="20sp" />

                <Button
                    android:id="@+id/bt_x"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="x"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_fx"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="f(x)="
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="25sp" />

                <Button
                    android:id="@+id/bt_modulo"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="|x|"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/bt_raiz"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="@string/raiz"
                    android:textAllCaps="false"
                    android:textColor="#000000"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_quadrado"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="xÂ²"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_potencia"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="x^n"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_euler"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="e^x"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_10x"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="*10^x"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="23sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/bt_log"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="log"
                    android:textAllCaps="false"
                    android:textColor="#000000"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_ln"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="ln"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_sinc"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="sinc"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_parentesis1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="("
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_parentesis2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text=")"
                    android:textColor="#000000"
                    android:textSize="30sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/bt_sin"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="sin"
                    android:textAllCaps="false"
                    android:textColor="#000000"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_cos"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="cos"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_tan"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="tan"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_pi"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="@string/pi"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_inv"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_circular"
                    android:text="^-1"
                    android:textColor="#000000"
                    android:textSize="23sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/bt_7"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="7"
                    android:textColor="#FBFAFA"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_8"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="8"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_9"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="9"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />


                <Button
                    android:id="@+id/bt_del"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_black"
                    android:text="DEL"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_AC"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_black"
                    android:text="AC"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">


                <Button
                    android:id="@+id/bt_4"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="4"
                    android:textColor="#FBFAFA"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_5"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="5"
                    android:textColor="#FBFAFA"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_6"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="6"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_multiplicacao"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_operacoes"
                    android:text="*"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_divisor"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_operacoes"
                    android:text="/"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/bt_1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="1"
                    android:textColor="#FBFAFA"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="2"
                    android:textColor="#FBFAFA"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="3"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_soma"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_operacoes"
                    android:text="+"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_subtraccao"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_operacoes"
                    android:text="-"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/bt_ponto"
                    android:layout_width="17dp"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="."
                    android:textColor="#FBFAFA"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_0"
                    android:layout_width="23dp"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/rounded_button"
                    android:text="0"
                    android:textColor="#FBFAFA"

                    android:textSize="30sp" />

                <Button
                    android:id="@+id/bt_table"
                    android:layout_width="61dp"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_operacoes"
                    android:text="Table"
                    android:textAllCaps="false"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />
                <Button
                    android:id="@+id/bt_plot"
                    android:layout_width="61dp"
                    android:layout_height="match_parent"
                    android:layout_margin="5sp"
                    android:layout_weight="1"
                    android:background="@drawable/bt_operacoes"
                    android:text="plot"
                    android:textAllCaps="false"
                    android:textColor="#F8F6F6"
                    android:textSize="30sp" />


            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

</RelativeLayout>